        android:theme="@style/Theme.Login.JobQues"
        tools:targetApi="31">
        <activity
            android:name=".ui.login.Login1"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Login.JobQues">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.login.Login2"
            android:theme="@style/Theme.Login.JobQues"/>

